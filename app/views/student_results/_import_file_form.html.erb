<div class="modal fade" id="imprtResultFile" tabindex="-1" role="dialog" aria-labelledby="imprtResultFile" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Import Student results file</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <%= form_tag(import_file_student_results_path, method: :post , multipart: true) do %>
        <div class="modal-body">
          <div class="field">
            <%= label_tag 'Choose Excel File' %>
            <%= file_field_tag :file, class: "", accept: '.xls, .xlsx', required: true %>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
          <%= submit_tag 'Save', class: "btn btn-primary"%>
        </div>
      <% end %>
    </div>
  </div>
</div>